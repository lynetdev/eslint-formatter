{
  "name": "@lynet/eslint-formatter",
  "version": "1.0.0-beta.0",
  "description": "Lynet ESLint formatter",
  "exports": "./dist/index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf ./dist"
  },
  "keywords": [
    "eslint",
    "eslint-formatter",
    "eslintformatter"
  ],
  "peerDependencies": {
    "eslint": ">= 5.x"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.9",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "3.2.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "simple-git": "^3.21.0",
    "synckit": "^0.8.6"
  }
}
